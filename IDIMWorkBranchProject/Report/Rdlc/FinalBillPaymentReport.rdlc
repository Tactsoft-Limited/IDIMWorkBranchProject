  <Code>Public Function ConvertToBangla(ByVal input As String) As String  
    Dim banglaDigits As String() = {"০", "১", "২", "৩", "৪", "৫", "৬", "৭", "৮", "৯"}
    Dim output As String = ""
    
    For i As Integer = 1 To Len(input)
        Dim ch As String = Mid(input, i, 1)
        
        If IsNumeric(ch) Then
            output &amp;= banglaDigits(CInt(ch))
        Else
            output &amp;= ch
        End If
    Next
    
    Return output
End Function


Public Function ConvertToBanglaDate(ByVal inputDate As Date) As String  
    ' Convert English digits to Bangla digits
    Dim banglaDigits As String() = {"০", "১", "২", "৩", "৪", "৫", "৬", "৭", "৮", "৯"}
    Dim banglaMonths As String() = {"জানুয়ারি", "ফেব্রুয়ারি", "মার্চ", "এপ্রিল", "মে", "জুন", "জুলাই", "আগস্ট", "সেপ্টেম্বর", "অক্টোবর", "নভেম্বর", "ডিসেম্বর"}
    
    ' Extract day, month, and year from the date
    Dim dayPart As String = CStr(Day(inputDate))
    Dim monthPart As String = banglaMonths(Month(inputDate) - 1)
    Dim yearPart As String = CStr(Year(inputDate))

    ' Convert day and year numbers to Bangla digits
    Dim banglaDay As String = ""
    Dim banglaYear As String = ""

    ' Convert Day to Bangla
    For i As Integer = 1 To Len(dayPart)
        banglaDay &amp;= banglaDigits(CInt(Mid(dayPart, i, 1)))
    Next

    ' Convert Year to Bangla
    For i As Integer = 1 To Len(yearPart)
        banglaYear &amp;= banglaDigits(CInt(Mid(yearPart, i, 1)))
    Next

    ' Return formatted Bangla date
    Return banglaDay &amp; " " &amp; monthPart &amp; " " &amp; banglaYear
End Function


Public Function ConvertToBanglaWords(ByVal amount As Double) As String
    Dim banglaOnkha As String() = {"", "এক", "দুই", "তিন", "চার", "পাঁচ", "ছয়", "সাত", "আট", "নয়"}
    Dim banglaScales As String() = {"", "হাজার", "লক্ষ", "কোটি"}

    If amount = 0 Then Return "শূন্য"

    Dim words As String = ""
    Dim crore As Integer = Int(amount / 10000000)
    Dim lakh As Integer = Int((amount Mod 10000000) / 100000)
    Dim thousand As Integer = Int((amount Mod 100000) / 1000)
    Dim hundred As Integer = Int((amount Mod 1000) / 100)
    Dim tens As Integer = Int((amount Mod 100) / 10)
    Dim units As Integer = Int(amount Mod 10)

    ' Handle Crore
    If crore &gt; 0 Then words &amp;= ConvertBelowThousand(crore) &amp; " কোটি "
    ' Handle Lakh
    If lakh &gt; 0 Then words &amp;= ConvertBelowThousand(lakh) &amp; " লাখ "
    ' Handle Thousand
    If thousand &gt; 0 Then words &amp;= ConvertBelowThousand(thousand) &amp; " হাজার "
    ' Handle Hundred
    If hundred &gt; 0 Then words &amp;= banglaOnkha(hundred) &amp; " শো "
    ' Handle Tens and Units
    If tens &gt;= 1 Then
        If tens = 1 And units &gt; 0 Then
            words &amp;= "ঊন" &amp; banglaOnkha(units) ' Handles numbers like 19, 29, etc.
        Else
            words &amp;= banglaOnkha(tens) &amp; " শো " &amp; banglaOnkha(units)
        End If
    Else
        words &amp;= banglaOnkha(units)
    End If

    Return Trim(words)
End Function

' Helper function to handle numbers below 1000
Private Function ConvertBelowThousand(ByVal num As Integer) As String
    Dim banglaOnkha As String() = {"", "এক", "দুই", "তিন", "চার", "পাঁচ", "ছয়", "সাত", "আট", "নয়"}
    Dim banglaTens As String() = {"", "দশ", "কুড়ি", "ত্রিশ", "চল্লিশ", "পঞ্চাশ", "ষাট", "সত্তর", "আশি", "নব্বই"}

    Dim words As String = ""

    Dim hundred As Integer = Int(num / 100)
    Dim tens As Integer = Int((num Mod 100) / 10)
    Dim units As Integer = num Mod 10

    If hundred &gt; 0 Then words &amp;= banglaOnkha(hundred) &amp; " শো "
    If tens &gt;= 1 Then
        If tens = 1 And units &gt; 0 Then
            words &amp;= "ঊন" &amp; banglaOnkha(units) ' Handles 11, 12, etc.
        Else
            words &amp;= banglaTens(tens) &amp; " " &amp; banglaOnkha(units)
        End If
    Else
        words &amp;= banglaOnkha(units)
    End If

    Return Trim(words)
End Function

